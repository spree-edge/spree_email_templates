                  "type": "html",
                  "values": {
                      "html": "<hr>\n<div class=\"container\">\n<div class=\"row\" >\n{% for lineitem in line_items %}\n  <div class=\"col-sm\" style=\"display: flex; justify-content: space-between; margin-top: 15px;\">\n  <div><b>{{ lineitem.name }}</b></div>\n  <div>{{ lineitem.quantity }} Ã— {{ lineitem.single_money }}</div>\n</div>\n  <br>\n<div class=\"col-sm\" style=\"display: flex; justify-content: space-between; margin-top: -16px;\">\n  <div style=\"font-size: 12px;\">({{ lineitem.options_text }})</div>\n  <div>{{ lineitem.display_amount }}</div>\n</div>\n  {% endfor %}\n</div>\n</div>\n<hr>\n\n<div class=\"container\">\n<div class=\"row\" >\n  <div class=\"col-sm\" style=\"display: flex; justify-content: flex-end; margin-top: 15px;\">\n<div style='margin-right: 50px;'> <b>Subtotal:</b> </div>\n  <div><b>${{ order.item_total }}</b></div>\n</div>\n</div>\n</div>\n\n<div class=\"container\">\n<div class=\"row\" >\n  <div class=\"col-sm\" style=\"display: flex; justify-content: flex-end; margin-top: 15px;\">\n    <div style='margin-right: 90px;'> <b>Shipping:</b></div> </div>\n    <div class=\"col-sm\" style=\"display: flex; justify-content: flex-end; margin-top: 15px;\">\n\t<div style='margin-right: 50px; margin-top: -10px;'>\n      {% for shipment in shipments %}\n    {{ shipment.shipping_method_name }}\n\t</div>\n\t\t<div style='margin-top: -10px;'><b>\n      ${{ shipment.cost }} </b>\n\t\t {% endfor %}\n\t</div>\n  </div>\n  </div>\n</div>\n\n<div class=\"container\">\n<div class=\"row\" >\n  <div class=\"col-sm\" style=\"display: flex; justify-content: flex-end; margin-top: 15px;\">\n    <div style='margin-right: 50px;'> <b>Order Total:</b></div>\n\t<div> <b>${{ order.total }}</b> </div>\n  </div></div></div>\n",
